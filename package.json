{
  "name": "wip",
  "version": "0.0.0-development",
  "description": "",
  "main": "index.js",
  "files": [
    "index.js",
    "lib"
  ],
  "scripts": {
    "coverage": "tap --coverage-report=html",
    "coverage:ci": "tap --coverage-report=text-lcov | coveralls",
    "deploy": "scripts/deploy.sh",
    "start": "probot run ./index.js",
    "now-start": "PRIVATE_KEY=$(echo $PRIVATE_KEY | base64 -d) npm start",
    "test": "tap --100 --coverage test/**/*-test.js",
    "posttest": "npm run lint",
    "semantic-release": "semantic-release",
    "lint": "standard"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wip/app.git"
  },
  "keywords": [
    "probot",
    "probot-plugin"
  ],
  "author": "Gregor Martynus (https://github.com/gr2m)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/wip/app/issues"
  },
  "homepage": "https://github.com/wip/app#readme",
  "dependencies": {
    "logdna-bunyan": "^1.0.1",
    "probot": "^7.0.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.1",
    "lodash.clonedeep": "^4.5.0",
    "lolex": "^2.7.0",
    "proxyquire": "^2.0.1",
    "semantic-release": "^15.0.0",
    "simple-mock": "^0.8.0",
    "smee-client": "^1.0.2",
    "standard": "^12.0.1",
    "tap": "^12.0.1"
  },
  "engines": {
    "node": ">=8.3.0",
    "npm": ">=5.3.0"
  },
  "release": {
    "prepare": [],
    "verifyConditions": [
      "@semantic-release/github"
    ],
    "publish": [
      "@semantic-release/github"
    ]
  },
  "now": {
    "name": "wip-beta",
    "alias": "wip-beta",
    "env": {
      "APP_ID": "@beta-app-id",
      "APP_NAME": "WIP (beta)",
      "LOG_LEVEL": "@beta-log-level",
      "LOGDNA_API_KEY": "@logdna-api-key",
      "NODE_ENV": "production",
      "PRIVATE_KEY": "@beta-private-key",
      "SENTRY_DSN": "@sentry-dsn",
      "WEBHOOK_SECRET": "@beta-webhook-secret"
    }
  }
}
